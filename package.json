{
  "name": "hoteldesign",
  "version": "1.0.0",
  "description": "Landing page for a hotel CSS design",
  "main": "index.js",
  "scripts": {
    "compile:sass": "node-sass sass/main.scss css/style.css -w",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.css -o css/style.prefix.css ",
    "compress:css": "node-sass css/style.css css/style2.css --output-style compressed"
  },
  "author": "Tiffany Luu",
  "license": "ISC",
  "dependencies": {
    "node-sass": "^5.0.0",
    "postcss-cli": "^8.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.3"
  }
}
